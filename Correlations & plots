install.packages("patchwork")
library(patchwork)
library(extrafont)
font_import()

loadfonts(device="win")       #Register fonts for Windows bitmap output

library(Hmisc)
library(corrplot)
ind_div = read.csv("GAD_corr_p5.csv", header=TRUE)
flattenCorrMatrix <- function(cormat, pmat) {
  ut <- upper.tri(cormat)
  data.frame(
    row = rownames(cormat)[row(cormat)[ut]],
    column = rownames(cormat)[col(cormat)[ut]],
    cor  =(cormat)[ut],
    p = pmat[ut]
    )
}

HCm <- as.matrix(HCid[,3:22])
GADm <- as.matrix(GADid[,3:22])

HCr <- rcorr(HCm)
HCfcm <- flattenCorrMatrix(HCr$r,HCr$P)

HCco <- HCr$r
HCpv <- HCr$P

GADr <- rcorr(GADm)
GADfcm <- flattenCorrMatrix(GADr$r,GADr$P)

GADco <- GADr$r
GADpv <- GADr$P

IDs <- as.matrix(ind_div[,3:22])
corr3 <- rcorr(IDs)
fcm3 <- flattenCorrMatrix(corr3$r,corr3$P)

coeff3 <- corr3$r
p.val3 <- corr3$P

col <- colorRampPalette(c("#BB4444", "#EE9988", "#FFFFFF", "#77AADD", "#4477AA"))
Holm <- corrplot(coeff3, method = "color", col = col(200), type = "upper", order = "hclust",
number.cex = .7,
         addCoef.col = "black", # Add coefficient of correlation
         tl.col = "black", tl.srt = 90, # Text label color and rotation
         # Combine with significance
         p.mat = p.val3, sig.level = 0.0003, insig = "blank", 
         # hide correlation coefficient on the principal diagonal
         diag = FALSE)		 

BH <- corrplot(coeff3, method = "color", col = col(200), type = "upper", order = "hclust",
number.cex = .7,
         addCoef.col = "black", # Add coefficient of correlation
         tl.col = "black", tl.srt = 90, # Text label color and rotation
         # Combine with significance
         p.mat = p.val3, sig.level = 0.025, insig = "blank", 
         # hide correlation coefficient on the principal diagonal
         diag = FALSE)		

p.adjust(corr1$P, method = c("holm", "BH"), n = 171)

