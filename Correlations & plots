  theme_set(theme_gray(base_size = 24)) +
  scale_size_manual(values=3)+ 
  scale_color_manual(values=c("#339900FF", "#6666FFFF")) +
  scale_fill_manual(values=c("#339900FF", "#6666FFFF")) +
  labs(x = 'Respiration Intensity', y = 'Postscan Anxiety')

